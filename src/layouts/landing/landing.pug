include ../../components/header/header
include ../../components/footer/footer

- const { header , footer } = require('@/layouts/landing/landing.json')

block variables

doctype html 
html(lang="ru")
	head
		meta(charset="UTF-8")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		meta(name="discription", content=discription)
		title #{title}
	body.landing
		+header(header)
		main.landing__main
			section.landing__container
				block content
		+footer(footer)